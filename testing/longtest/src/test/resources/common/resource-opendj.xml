						<expression>
						<path>declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance/10000000-0000-0000-0000-000000000003";
                            $account/attributes/ri:employeeNumber</path>
						</expression>
					</q:equal>
					<q:not>
						<q:equal>
							<q:path>c:employeeNumber</q:path>
							<q:value/>
						</q:equal>
					</q:not>
				</q:and>
			</correlation>
			
